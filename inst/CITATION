citHeader("To cite Xmisc in publications use:")

citEntry(
  entry="Manual",
  title = "Xmisc: Xiaobei's miscellaneous classes and functions",
  author = personList(
    as.person("Xiaobei Zhao")
    ),
  year = "2014",
  url = "http://CRAN.R-project.org/package=Xmisc",
  note = "R package version 0.2.1",
  textVersion=
  paste(
    "Zhao, Xiaobei",
    "Xmisc: Xiaobei's miscellaneous classes and functions", 
    "R package version 0.2.1",
    "2014",
    "http://CRAN.R-project.org/package=Xmisc",
    sep=". "
    ),
  )
